function Vaporwaveify(text)
function wide(a)
if a < '!' or a > '~' then return a end
if a==' ' then return '  ' end 
a = a:byte() + 160
if a<256 then return string.char(239, 188, a-64) end
return string.char(239, 189, a-128) end
return(text:gsub('.',wide)) end

function doanim(id, timepos, playfade, stopfade, stoptime)
local atanim = INST'Animation'
atanim.AnimationId = 'rbxassetid://'..id
local at1 = noid:LoadAnimation(atanim)
at1:Play(playfade or .1, 1, 1)
at1.TimePosition = timepos or 0
task.delay(stoptime, function()
at1:Stop(stopfade or .1)
end) end

function chatfunc(text)
local chat = coroutine.wrap(function()
if paster:FindFirstChild'TalkingBillBoard' ~= nil then
paster:FindFirstChild'TalkingBillBoard':Destroy()
end

local bill = INST('BillboardGui', paster)
bill.Size = UDim2.new(.15, 0, .05, 0)
bill.StudsOffsetWorldSpace = Vector3.new(0, 3, 0) 
bill.Adornee = head
bill.Name = 'TalkingBillBoard'
bill.AlwaysOnTop = true

local hehe = INST('TextLabel', bill)
hehe.BackgroundTransparency = 1
hehe.BorderSizePixel = 0
hehe.Text = ''
hehe.Font = 'Garamond'
hehe.TextSize = 40
hehe.TextStrokeTransparency = 0
hehe.Size = UDim2.new(1, 0, 1, 0)

coroutine.resume(coroutine.create(function()
while hehe ~= nil and hehe.Parent ~= nil do
task.wait()
local v = math.random(1, 255)
local col = Color3.fromRGB(v, v, v)
hehe.TextColor3 = col
hehe.TextStrokeColor3 = col
end end))

for i = 1, #text, 1 do
task.wait()
hehe.Text = string.sub(text,1,i) end

task.wait(1.2)

local sound = INST('Sound', soundsparent)
sound.SoundId = 'rbxassetid://749189256'
sound.Volume = 4
sound.PlaybackSpeed = math.random(5, 15) / 10
sound:Play()
DE:AddItem(sound, 4)

for i = 1, 100 do
task.wait()
hehe.Text = hehe.Text.. string.char(math.random(0,126))
hehe.Rotation = hehe.Rotation - .8
hehe.TextStrokeTransparency = hehe.TextStrokeTransparency + .01
hehe.TextTransparency = hehe.TextTransparency + .01 end

bill:Destroy() end)
chat() end

