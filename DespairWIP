repeat task.wait() until G2L['VButton_9']

local cooldowns = { lmb = true }

G2L['VButton_9'].MouseButton1Click:Connect(function()
if not cooldowns.lmb then return end

local hitbox = INST'Part'
hitbox.Size = Vector3.new(10,1,10)
hitbox.Anchored = true
hitbox.CanCollide = false
hitbox.Transparency = .5
hitbox.Material = Enum.Material.ForceField
hitbox.Color = Color3.fromRGB(255, 0, 0)
hitbox.Parent = WS

local con
con = game:service'RunService'.PreRender:Connect(function()
hitbox.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 6

for ez, code in next, game['Players']:GetPlayers() do
if code ~= yourself and code.Character
then local torso = code.Character:FindFirstChild'Torso'
local hitboxrange = (hitbox.Position - torso.Position).Magnitude
if hitboxrange <= 5.5 then
local cd = code.Character:FindFirstChild'hitbox_cooldown'
if not cd then
cd = INST'BoolValue'
cd.Name = 'hitbox_cooldown'
cd.Value = false
cd.Parent = code.Character end
if cd.Value == false
then cd.Value = true
RepS.DiamondHit:FireServer(torso)
task.delay(1.25, function()
if cd and cd.Parent
then cd.Value = false
end end) end end end end end)

task.delay(.5, function()
local hitboxfade = game:service'TweenService':Create(hitbox, TweenInfo.new(2), {Transparency = 1})
hitboxfade:Play()
task.delay(.7, function()
if con then con:Disconnect() end
if hitbox and hitbox.Parent then hitbox:Destroy() end
end) end)

doanim('13544664658', .4, .2, .2, 2)
docooldown('Enlighten!', .8, cooldowns, 'lmb')
end) 

