function visualize()
if getgenv().visualizerexists then return end
getgenv().visualizerexists = true
if not csound then return end

local parts = {}
local VisualizerNNN = INST('Model', WS)
VisualizerNNN.Name = 'Visualizer'

for i = 1, 48 do
local part = INST'Part'
part.Size = Vector3.new(.3, 2, .3)
part.Anchored = true
part.Material = Enum.Material.Neon
part.Color = Color3.fromRGB(245, 245, 245)
part.CanCollide = false
part.Parent = VisualizerNNN
parts[i] = part end

local smooth, lasty = {}, {}
for i = 1, 48 do
smooth[i] = 2
lasty[i] = 5 end

local maxheight = humanoidrootpart.Position.Y - 3.3

spawn(function()
local t = 0
while csound and csound.Parent do
local dt = task.wait()
t += dt * vizspeed

local loud = (csound.PlaybackLoudness / 1000) * 20

for e, bar in next, parts do
local active = math.random() > .5
local target = active and math.clamp(loud, .5, 15) or 0.5
smooth[e] += (target - smooth[e]) * .25

local angle = (e / 48) * math.pi * 2 + t * .2
local xpos = humanoidrootpart.Position.X + math.cos(angle) * 10
local zpos = humanoidrootpart.Position.Z + math.sin(angle) * 10

bar.Size = Vector3.new(.3, smooth[e], .3)
bar.CFrame = CFrame.new(xpos, maxheight + smooth[e] / 2, zpos) end

if not csound.IsPlaying and not draggy
then break
end end
print 'hi'
end) end

spawn(function()
runservice.PostSimulation:Connect(function()
vizspeed += (targetvizspeed - vizspeed) * .12
end) end)

