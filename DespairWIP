--// Essentials \\--

local Frame_Speed = 60

local ArtificialHB = INST'BindableEvent'
ArtificialHB.Name = "ArtificialHeartbeat"

local tf = 0
local allowframeloss = true
local tossremainder = true
local lastframe = tick()
local frame = 1 / Frame_Speed
ArtificialHB:Fire()

game.GetService(game, 'RunService').Stepped:connect(function(s, p)
tf = tf + s
if tf >= frame then
if allowframeloss then
ArtificialHB:Fire()
lastframe = tick() else
for i = 1, math.floor(tf / frame) do
ArtificialHB:Fire() end
lastframe = tick() end
if tossremainder then
tf = 0 else
tf = tf - frame * math.floor(tf / frame)
end end end)

function swait(amount: number?, mode: string?): boolean
mode = mode or 'f'

if mode == 'f'
then if not amount or amount == 0 then
ArtificialHB.Event:Wait() else
for _ = 1, amount do
ArtificialHB.Event:Wait()
end end
return true
elseif mode == 's'
then local t = tick() + amount
repeat
ArtificialHB.Event:Wait()
until tick() >= t
return true
elseif mode == 'm'
then local t = tick() + amount * 60
repeat
ArtificialHB.Event:Wait()
until tick() >= t
return true; else
warn('Invalid method '..tostring(mode))
return false
end end

--[[
S: Second
M: Minute
F: Frame
]]--

