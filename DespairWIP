function updatebar(posX)
local barpos = bar.AbsolutePosition.X
local barsize = bar.AbsoluteSize.X
local relative = math.clamp(posX - barpos, 0, barsize)
local ntime = (relative / barsize) * csound.TimeLength
csound.TimePosition = ntime
fillbar.Size = UDim2.new(relative / barsize, 0, 1, 0)
indicator.Position = UDim2.new(relative / barsize, 0, .5, 0) end

function dragb(input)
if not csound then return end
draggy = true
dragi = input
csound:Pause() 
targetvizspeed = 0 end

indicator.InputBegan:Connect(dragb)
UIS.InputEnded:Connect(function(input)
if draggy and dragi and input == dragi then
draggy = false
dragi = nil
if csound then csound:Resume() end
targetvizspeed = 1 end end)

runservice.PreRender:Connect(function()
if draggy and dragi then local posX
if dragi.UserInputType == Enum.UserInputType.MouseMovement then
posX = UIS:GetMouseLocation().X
elseif dragi.UserInputType == Enum.UserInputType.Touch then
posX = dragi.Position.X
end
if posX then updatebar(posX) end
elseif csound and not draggy then
local progress = csound.TimePosition / csound.TimeLength
fillbar.Size = UDim2.new(progress, 0, 1, 0)
indicator.Position = UDim2.new(progress, 0, .5, 0)
end end) end)

function CreatePartb(formFactor, parent, material, reflectance, transparency, color, name, size, anchored)
local v = INST'Part'
v.FormFactor = formFactor
v.Size = size
v.BrickColor = BrickColor.new(color)
v.Material = material
v.Reflectance = reflectance
v.Transparency = transparency
v.Anchored = anchored or false
v.CanCollide = false
v.Locked = true
v.Name = name
v.Parent = parent return v end

function CreateWeldOrSnapOrMotor(type, part0, part1, part2, c0, c1)
local v = INST(type)
v.Part0 = part0
v.Part1 = part2 or part1
v.C0 = c0 or CFrame.new()
v.C1 = c1 or CFrame.new()
v.Parent = part0 return v end

